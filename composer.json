{
    "name": "escapework/laramedias",
    "description": "A Laravel package that integrates Glide for easily manage medias on your project.",
    "license": "MIT",

    "require": {
        "escapework/laravel-steroids": "0.5.*",
        "league/glide": "1.0.*"
    },

    "autoload": {
        "psr-4": {
            "EscapeWork\\LaraMedias\\": "src/"
        },

        "files": [
            "src/functions.php"
        ]
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.4.*-dev"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
